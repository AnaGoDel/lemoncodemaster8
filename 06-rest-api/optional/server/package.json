{
  "name": "characters-api",
  "version": "1.0.0",
  "description": "Characters Rick and Morty API",
  "main": "index.js",
  "scripts": {
    "mock-server": "json-server --routes ./config/routes.json --watch src/mock-data/characters-data.json --middlewares ./config/characters.middleware.js --no-delete-cascade",
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "nodemon --exec babel-node --extensions \".ts\" src/index.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "author": "Lemoncode",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@types/express": "^4.17.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "apollo-server-express": "^2.18.2",
    "express": "^4.17.1",
    "json-server": "^0.16.1",
    "regenerator-runtime": "^0.13.7"
  }
}
